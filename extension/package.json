{
  "name": "snipe",
  "displayName": "Snipe",
  "description": "Real-time cross-file semantic analysis on unsaved code",
  "version": "0.1.0",
  "publisher": "snipe",
  "engines": { "vscode": "^1.85.0" },
  "categories": ["Linters", "Other"],
  "activationEvents": ["onLanguage:python", "onLanguage:c", "workspaceContains:**/*.py", "workspaceContains:**/*.c"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "snipe.openGraph",
        "title": "Snipe: Open Repo Knowledge Graph"
      },
      {
        "command": "snipe.refreshRepo",
        "title": "Snipe: Refresh Repository Symbols"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {}
}
